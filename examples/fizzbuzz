extern printf(fmt: *i8, ...) -> i32;
extern scanf(fmt: *i8, ...) -> i32;

fn main() -> i32
{
  let mut n: u32;
  printf("> ");
  if (scanf("%u", &n) != 1)
    ret 1;

  for (let mut i = 1; i <= n; ++i) {
    if (i % 15 == 0)
      printf("FizzBuzz\n");
    else if (i % 3 == 0)
      printf("Fizz\n");
    else if (i % 5 == 0)
      printf("Buzz\n");
    else
      printf("%d\n", i);
  }
}
