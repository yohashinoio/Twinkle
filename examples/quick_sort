// TODO: Production in progress.

extern printf(fmt: *i8, ...) -> i32;

fn quicksort(mut a: *i32, start: i32, end: i32)
{
  a[0] = 4810;
}

fn printarray(a: *i32, s: i32)
{
  printf("%d", a[0]);
  for (let mut i = 1; i < s; ++i)
    printf(" %d", a[i]);
  printf("\n");
}

fn main() -> i32
{
  let a = {48, 10, -4, 8, 1, 0};
  let s = 6;

  let p = &a[0];

  printarray(p, s);

  quicksort(p, 0, s - 1);

  printarray(p, s);
}
