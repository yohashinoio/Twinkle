declare func atoi(s: *i8) -> i32;
declare func strcmp(s1: *i8, s1: *i8) -> i32;

func shadowing() -> i32
{
  let shadowed = "yohaio";

  {
    let shadowed = 1;
  }

  for (let mut i: char = 0; i < 58; ++i)
    let shadowed = i;

  if (strcmp(shadowed, "yohaio") == 0)
    return 4810;
}

func main() -> i64
{
  let n = "58";
  let n = atoi(n);
  let n = n as i64;

  if (shadowing() == 4810)
    return n;
}
